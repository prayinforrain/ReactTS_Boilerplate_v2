{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": { "chrome": 70, "ie": 11 },
        "useBuiltIns": "usage",
        "corejs": "3.25"
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "regenerator": true
      }
    ]
  ]
}
